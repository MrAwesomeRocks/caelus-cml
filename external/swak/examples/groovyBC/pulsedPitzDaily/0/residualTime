FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    inlet       
    {
        type            fixedValue;
        value uniform 0;
    }
    outlet       
    {
        type            inletOutlet;
        value uniform 0;
        inletValue uniform 0;
    }

    frontAndBack    
    {
        type            empty;
    }

    ".+"           
    {
        type            zeroGradient;
    }
}

