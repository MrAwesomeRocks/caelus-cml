FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 101325;

boundaryField
{
    floor
    {
        type            buoyantPressure;
        value           $internalField;
    }
    ceiling
    {
        type            buoyantPressure;
        value           $internalField;
    }
    inlet
    {
        type            buoyantPressure;
        value           $internalField;
    }
    outlet
    {
        type            buoyantPressure;
        value           $internalField;
    }
    fixedWalls
    {
        type            empty;
    }
    "baffle1Wall.*"
    {
        type           calculated;
    }
}


