FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    floor
    {
        type            buoyantPressure;
        rho             rhok;
        value           uniform 0;
    }

    ceiling
    {
        type            buoyantPressure;
        rho             rhok;
        value           uniform 0;
    }

    fixedWalls
    {
        type            buoyantPressure;
        rho             rhok;
        value           uniform 0;
    }
}

