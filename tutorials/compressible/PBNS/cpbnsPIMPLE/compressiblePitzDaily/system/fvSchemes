FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear skewCorrected;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss BarthJespersenV grad(U) skewCorrected;
    div(phid,p)     Gauss BarthJespersen  grad(p) skewCorrected;
    div(phi,KE)     Gauss linear skewCorrected;
    div(phi,h)      Gauss BarthJespersen  grad(h) skewCorrected;
    div(phi,k)      Gauss BarthJespersen  grad(k) skewCorrected;
    div((muEff*dev2(T(grad(U))))) Gauss linear skewCorrected;
    div(((muEff*grad(U))&U)) Gauss linear skewCorrected;
    div(((muEff*dev2(T(grad(U))))&U)) Gauss linear skewCorrected;
}

laplacianSchemes
{
    default         Gauss linear corrected skewCorrected;
}

interpolationSchemes
{
    default         linear skewCorrected;
    reconstruct(T)  Minmod grad(T);
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p               ;
}


// ************************************************************************* //
